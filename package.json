{
  "name": "gm-ebus",
  "version": "3.0.1",
  "description": "State manager",
  "main": "./lib/index.js",
  "scripts": {
    "build-cjs": "rimraf lib && babel ./src -d lib",
    "dev": "rimraf lib && babel ./src -d lib --watch",
    "perf": "mocha --compilers js:babel-core/register \"./src/**/*.__perf__.js\"",
    "test:file": "mocha --compilers js:babel-core/register --require ./src/__test__/setup.js",
    "test": "mocha --compilers js:babel-core/register --require ./src/__test__/setup.js \"./src/**/*.__test__.js\"",
    "example": "webpack-dev-server --config ./example/webpack.config.js --inline --hot",
    "example:production": "cross-env NODE_ENV=production webpack-dev-server --config ./example/webpack.config.js",
    "prepublishOnly": "yarn run test && yarn run build-cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mariangibala/gm-ebus.git"
  },
  "keywords": [
    "pub/sub",
    "flux"
  ],
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "private": false,
  "author": "Marian Gibala",
  "license": "MIT",
  "homepage": "https://github.com/mariangibala/gm-ebus",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-core": "6.25.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "7.1.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-env": "1.5.2",
    "babel-preset-react": "6.11.1",
    "chai": "4.1.1",
    "cross-env": "5.1.1",
    "eslint": "5.13.0",
    "eslint-plugin-react": "7.12.4",
    "html-webpack-plugin": "3.2.0",
    "mocha": "3.5.0",
    "q": "1.5.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "rimraf": "2.6.1",
    "sinon": "4.0.1",
    "uglifyjs-webpack-plugin": "2.1.1",
    "webpack": "4.29.3",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14"
  }
}
